service mycamera-provider-2-4 /vendor/bin/hw/vendor.masaki.hardware.camera.provider@2.4-service
    interface android.hardware.camera.provider@2.4::ICameraProvider mycamera
    class hal
    user cameraserver
    group audio camera input drmrpc
    ioprio rt 4
    capabilities SYS_NICE
    writepid /dev/cpuset/camera-daemon/tasks /dev/stune/top-app/tasks

